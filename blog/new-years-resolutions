<script>

	function buildIframe(vimeoId) {
		var iframe = document.createElement('iframe');
			iframe.onload = function() {  }; // before setting 'src'
			iframe.src = 'https://player.vimeo.com/video/' + vimeoId + '?autoplay=1&muted=1'
			iframe.width=640
			iframe.height=360
			iframe.style="box-sizing: border-box;"
			iframe.frameborder=0
			iframe.setAttribute('webkitallowfullscreen', '')
			iframe.setAttribute('mozallowfullscreen', '')
			iframe.setAttribute('allowfullscreen', '')

		return iframe
	}

	function buildCookiePrompt (vimeoId) {
		return cookiePrompt = $('<div>', {id: 'cookie-prompt-' + vimeoId, class: 'cookie-prompt'})
			.css({opacity: 0, display: 'flex'})
			.append($('<p>').text('Vimeo player uses third-party cookies'))
			.append($('<div>')
				.append($('<span>', {class: 'button', text: 'I Understand', onclick: 'swapVideoContent(' + vimeoId + ')'}))
			)
	}

	function swapVideoContent(vimeoId, skipCookiePrompt) {

		var $div = $('#div-' + vimeoId)
		$div.css({height: ($div.innerHeight() - 3) + 'px', width: $div.innerWidth() + 'px'})
			.removeClass('cookie-prompt-active')
			.find('img')
			.css({ filter: '', position: 'absolute', top: 0, right: 0, bottom: 0, left: 0, 'z-index': '-1'})

		if (skipCookiePrompt) {
			var iframe = buildIframe(vimeoId)
			$div.find('.title').css({opacity: 0})
			$div.find('.play-button').fadeOut(200, function() {
				$div.append(iframe);
				$div.css({height: '', width: ''})
			})
		} else {
			setLocalStorageToken()
			$div.find('.cookie-prompt').fadeOut(500, function() {
				var iframe = buildIframe(vimeoId)
				$div.append(iframe);
				$div.css({height: '', width: ''})
			})
		}
	}

	function displayCookiePrompt (vimeoId) {
		var cookiePrompt = buildCookiePrompt(vimeoId)

		var $div = $('#div-' + vimeoId)
		$('#image-' + vimeoId).css({filter: "brightness(0.2)"}).removeAttr('onclick')
		if($div) {
			$div.find('.play-button').fadeOut(500)
			$div.find('.title').css({opacity: 0})
			$div.append(cookiePrompt.fadeTo(1000, 1, function() {
				$div.addClass('cookie-prompt-active')
			}))
		}
	}

	function setLocalStorageToken() {
		localStorage.setItem('acceptVimeoCookies', new Date())
	}

	function handleVideoClick(vimeoId) {
		if (localStorage.getItem('acceptVimeoCookies')) {
			swapVideoContent(vimeoId, true)
		} else {
			displayCookiePrompt(vimeoId)
		}
	}

	</script>

